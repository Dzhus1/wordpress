msgid "Payment declined order status"
msgstr "Статус заказа при отказе"

#: woocommerce-fondy.php:263
msgid "Order status when payment was declined."
msgstr "Изменить статус заказа при отказе"

#: woocommerce-fondy.php:275
msgid "Default status"
msgstr "Стандартный статус"

#: woocommerce-fondy.php:290
msgid "Fondy.eu"
msgstr "FONDY.eu"

